<% include ../header %>
<div id="header">
    <h1 >Overland It!</h1>

</div>
<div layout="row" layout-sm="column" layout-wrap>    
<% blobs.forEach(function(blob){ %>
<div flex="30" flex-sm="95" layout-wrap>
 <md-card>
    <a href=<%='/blobs/' + blob._id %> >
        <img src="<%= blob.url %>" class="md-card-image">
    </a>
            
  <md-card-content>
    <h2><%= blob.title %></h2>
    <h3>$<%= blob.price %></h3>
    <h3><%= blob.location %></h3>
  </md-card-content>
    
 </md-card>
</div>
<% }) %>

<% include ../footer %>
